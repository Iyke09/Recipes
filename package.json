{
  "name": "more-recipe",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "wp": "webpack-dev-server --hot --history-api-fallback --open",
    "test": "cross-env NODE_ENV=test nyc mocha server/test/test.js ",
    "test:db": "set NODE_ENV=test&&sequelize db:migrate:undo:all&&sequelize db:migrate && cross-env NODE_ENV=test nyc mocha server/test/test.js",
    "s:test": "set NODE_ENV=test-l&& mocha --compilers js:babel-core/register --timeout 10000 server/test/test.js",
    "heroku-postbuild": "sequelize db:migrate:undo:all&&sequelize db:migrate && npm run build",
    "migrate": "sequelize db:migrate:undo:all&&sequelize db:migrate",
    "start": "nodemon --exec babel-node server/bin/www",
    "build:webpack": "set NODE_ENV=production && webpack -p --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test-client": "NODE_ENV=production mocha './tests/**/*.spec.js' --compilers js:babel-core/register",
    "clean": "rimraf dist",
    "start:dev": "concurrently --kill-others \"nodemon --exec babel-node server/bin/www\" \" npm start --prefix ./client\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "shc": "start coverage/lcov-report/index.html"
  },
  "engines": {
    "node": "6.10.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "",
  "license": "ISC",
